{
  "name": "@soc-compliance/e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for SOC Compliance Platform",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:auth": "jest --testMatch='**/01-auth-flow.e2e-spec.ts'",
    "test:org": "jest --testMatch='**/02-organization-setup.e2e-spec.ts'",
    "test:policy": "jest --testMatch='**/03-policy-management.e2e-spec.ts'",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "axios": "^1.6.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}