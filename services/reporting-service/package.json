{
  "name": "@soc-compliance/reporting-service",
  "version": "1.0.0",
  "description": "Compliance Reporting Service for SOC Platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json --runInBand",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d ./src/data-source.ts",
    "start:direct": "node dist/src/main.js"
  },
  "dependencies": {
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@soc-compliance/auth-common": "^1.0.0",
    "@soc-compliance/contracts": "^1.0.0",
    "@soc-compliance/events": "^1.0.0",
    "@soc-compliance/http-common": "^1.0.0",
    "@soc-compliance/monitoring": "file:../../packages/monitoring",
    "@types/xlsx": "^0.0.35",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cron": "^2.4.3",
    "cron-parser": "^4.9.0",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "minio": "^7.1.3",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.0",
    "puppeteer": "^21.6.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.9",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
