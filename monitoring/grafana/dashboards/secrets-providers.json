{
  "dashboard": {
    "id": null,
    "title": "Secrets Management - Provider Health Dashboard",
    "tags": ["secrets", "providers", "health", "infrastructure"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Provider Health Overview",
        "type": "table",
        "targets": [
          {
            "expr": "secrets_provider_health_status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Unhealthy", "color": "red" } }, "type": "value" },
              { "options": { "1": { "text": "Healthy", "color": "green" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Provider Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(secrets_provider_response_time_seconds_bucket[5m])) by (le, provider))",
            "refId": "A",
            "legendFormat": "p50 - {{provider}}"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(secrets_provider_response_time_seconds_bucket[5m])) by (le, provider))",
            "refId": "B",
            "legendFormat": "p95 - {{provider}}"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(secrets_provider_response_time_seconds_bucket[5m])) by (le, provider))",
            "refId": "C",
            "legendFormat": "p99 - {{provider}}"
          }
        ],
        "gridPos": { "h": 9, "w": 12, "x": 0, "y": 8 },
        "yAxes": [{ "unit": "s", "min": 0 }, { "unit": "short" }]
      },
      {
        "id": 3,
        "title": "Provider Error Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(secrets_provider_errors_total[5m])) by (provider, error_type)",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 9, "w": 12, "x": 12, "y": 8 },
        "yAxes": [{ "unit": "ops", "min": 0 }, { "unit": "short" }]
      },
      {
        "id": 4,
        "title": "AWS Secrets Manager Health",
        "type": "stat",
        "targets": [
          {
            "expr": "secrets_provider_health_status{provider=\"aws-secrets-manager\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Unhealthy", "color": "red" } }, "type": "value" },
              { "options": { "1": { "text": "Healthy", "color": "green" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 17 }
      },
      {
        "id": 5,
        "title": "HashiCorp Vault Health",
        "type": "stat",
        "targets": [
          {
            "expr": "secrets_provider_health_status{provider=\"hashicorp-vault\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Unhealthy", "color": "red" } }, "type": "value" },
              { "options": { "1": { "text": "Healthy", "color": "green" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 17 }
      },
      {
        "id": 6,
        "title": "Local Provider Health",
        "type": "stat",
        "targets": [
          {
            "expr": "secrets_provider_health_status{provider=\"local-encrypted\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Unhealthy", "color": "red" } }, "type": "value" },
              { "options": { "1": { "text": "Healthy", "color": "green" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 17 }
      },
      {
        "id": 7,
        "title": "Docker Secrets Health",
        "type": "stat",
        "targets": [
          {
            "expr": "secrets_provider_health_status{provider=\"docker-secrets\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Unhealthy", "color": "red" } }, "type": "value" },
              { "options": { "1": { "text": "Healthy", "color": "green" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 17 }
      },
      {
        "id": 8,
        "title": "Provider Operations Load",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(secrets_access_total[5m])) by (provider)",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 9, "w": 12, "x": 0, "y": 25 },
        "yAxes": [{ "unit": "ops", "min": 0 }, { "unit": "short" }]
      },
      {
        "id": 9,
        "title": "Circuit Breaker States",
        "type": "graph",
        "targets": [
          {
            "expr": "secrets_circuit_breaker_state",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "options": { "0": { "text": "Closed", "color": "green" } }, "type": "value" },
              { "options": { "1": { "text": "Open", "color": "red" } }, "type": "value" },
              { "options": { "2": { "text": "Half-Open", "color": "yellow" } }, "type": "value" }
            ]
          }
        },
        "gridPos": { "h": 9, "w": 12, "x": 12, "y": 25 },
        "yAxes": [{ "unit": "short", "min": 0, "max": 2 }, { "unit": "short" }]
      },
      {
        "id": 10,
        "title": "Provider Rotation Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(secrets_rotation_duration_seconds_bucket[5m])) by (le, provider))",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 9, "w": 24, "x": 0, "y": 34 },
        "yAxes": [{ "unit": "s", "min": 0 }, { "unit": "short" }]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}
