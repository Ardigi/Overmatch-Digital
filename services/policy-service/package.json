{
  "name": "@soc-compliance/policy-service",
  "version": "1.0.0",
  "description": "Policy Management and Policy-as-Code Service",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.unit.js",
    "test:unit": "jest --config jest.config.unit.js",
    "test:watch": "jest --config jest.config.unit.js --watch",
    "test:cov": "jest --config jest.config.unit.js --coverage",
    "test:integration": "jest --config jest.config.integration.js",
    "test:e2e": "jest --config jest.config.e2e.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "type-check": "tsc --noEmit",
    "migration:generate": "npm run typeorm migration:generate -- -d src/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/data-source.ts",
    "start:direct": "node dist/src/main.js",
    "verify:typescript": "node verify-typescript.js",
    "verify:tests": "node verify-tests.js",
    "verify:bypasses": "node verify-no-bypasses.js",
    "verify:completion": "node verify-completion.js",
    "verify:monitoring": "node scripts/verify-monitoring.js",
    "verify": "npm run verify:completion"
  },
