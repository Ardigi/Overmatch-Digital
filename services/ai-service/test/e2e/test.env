# AI Service E2E Test Environment Variables

# Node Environment
NODE_ENV=test

# Service Configuration
PORT=3011

# PostgreSQL Database Configuration - Test Database
DB_HOST=127.0.0.1
DB_PORT=5433
DB_USERNAME=test_user
DB_PASSWORD=test_pass
DB_NAME=soc_ai_test

# JWT Configuration
JWT_SECRET=test-jwt-secret-key

# MongoDB Configuration - Test MongoDB
MONGO_URI=mongodb://127.0.0.1:27018/test_ai

# Elasticsearch Configuration - Test Elasticsearch
ELASTICSEARCH_NODE=http://127.0.0.1:9201
ELASTICSEARCH_USERNAME=
ELASTICSEARCH_PASSWORD=
ELASTICSEARCH_INDEX_PREFIX=soc_ai_test

# Kafka Configuration - Test Kafka
KAFKA_BROKERS=127.0.0.1:9093
KAFKA_CLIENT_ID=ai-service-test
KAFKA_GROUP_ID=ai-service-group-test
DISABLE_KAFKA=true

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6380
REDIS_PASSWORD=test_redis_pass

# AI Provider Configuration
AI_PROVIDER=mock
OPENAI_API_KEY=test-key
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# Model Configuration
MODEL_STORAGE_PATH=/tmp/ai-models-test
ENABLE_MODEL_VERSIONING=true
MODEL_RETENTION_COUNT=3
DEFAULT_MODEL_TIMEOUT_MS=30000

# Analysis Configuration
MAX_CONCURRENT_ANALYSES=5
ANALYSIS_QUEUE_NAME=ai-analysis-test
ANALYSIS_RESULT_TTL_DAYS=90

# Risk Assessment
RISK_SCORE_THRESHOLD_HIGH=0.8
RISK_SCORE_THRESHOLD_MEDIUM=0.5
RISK_SCORE_THRESHOLD_LOW=0.2
ENABLE_REAL_TIME_RISK_SCORING=true

# Control Effectiveness
CONTROL_EFFECTIVENESS_MIN_SAMPLES=5
CONTROL_EFFECTIVENESS_CONFIDENCE_LEVEL=0.95
ENABLE_PREDICTIVE_ANALYTICS=true

# Anomaly Detection
ANOMALY_DETECTION_ALGORITHM=isolation_forest
ANOMALY_CONTAMINATION_FACTOR=0.1
ANOMALY_MIN_SAMPLES=100
ENABLE_ADAPTIVE_THRESHOLDS=true

# Natural Language Processing
NLP_ENABLED=true
NLP_LANGUAGES=en,es,fr
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_ENTITY_EXTRACTION=true

# Training Configuration
ENABLE_CONTINUOUS_LEARNING=false
TRAINING_BATCH_SIZE=32
TRAINING_EPOCHS=10
VALIDATION_SPLIT=0.2

# Data Privacy
ENABLE_PII_DETECTION=true
PII_REDACTION_ENABLED=true
DATA_ANONYMIZATION_ENABLED=true

# External Service URLs (Test Instances)
AUTH_SERVICE_URL=http://localhost:3001
CONTROL_SERVICE_URL=http://localhost:3004
EVIDENCE_SERVICE_URL=http://localhost:3005
AUDIT_SERVICE_URL=http://localhost:3008

# Performance
CACHE_ENABLED=true
CACHE_TTL_SECONDS=3600
MAX_CACHE_SIZE_MB=100

# Security
ENABLE_MODEL_ENCRYPTION=false
MODEL_SIGNING_KEY=test-model-key

# Testing
ENABLE_TEST_MODE=true
USE_MOCK_AI_PROVIDER=true
MOCK_RESPONSE_DELAY_MS=100
DETERMINISTIC_RESULTS=true